
.title_1{
		color: var(--gray-900, #18181B);
		-webkit-font-feature-settings: 'clig' off, 'liga' off;
		        font-feature-settings: 'clig' off, 'liga' off;
		font-family: Manrope;
		font-size: 44rpx;
		font-style: normal;
		font-weight: 800;
		line-height: 60rpx; /* 136.364% */
		letter-spacing: -0.8rpx;
}
.frame{
		display: flex;
		width: 646rpx;
		flex-direction: column;
		align-items: flex-start;
		gap: 40rpx;
		padding-bottom: 100rpx; /* 假设底部导航栏的高度为50px */
}
.combined-text{
		display: inline-flex;
		align-items: center;
		justify-content: center;
}
.answer_text{
		color: #000;
		font-family: Inter;
		font-size: 28rpx;
		font-style: normal;
		font-weight: 400;
		line-height: normal;
		 display: inline-block;
		letter-spacing: 0.56rpx;
}
.title_2{
		color: var(--gray-600, #52525B);
		-webkit-font-feature-settings: 'clig' off, 'liga' off;
		        font-feature-settings: 'clig' off, 'liga' off;
		font-family: Manrope;
		font-size: 32rpx;
		font-style: normal;
		font-weight: 500;
		line-height: 48rpx; /* 150% */
		letter-spacing: -0.8rpx;
}
.title_frame{
		display: flex;
		width: 646rpx;
		flex-direction: column;
		align-items: flex-start;
		gap: 24rpx;
}
.response_result{
		width: 606rpx;
		padding: 20rpx;
		border: 2rpx solid #ccc;
		border-radius: 20rpx;
		background-color: #f8f8f8;
}
.response_result image{
		border-radius: 20rpx;
		width: 100%;
}
.bottom-section {
	        position: absolute;
	        bottom: 0;
	        left: 0;
	        right: 0;
}
.button_item{
		width: 606rpx;
		height: 214rpx;
		flex-shrink: 0;
		display: flex;
		 align-items: center; /* 垂直居中 */
		border-radius: 40rpx;
		background: #F5F6FA;
		padding-left: 80rpx;
		
		position: relative;
}
.text_frame{
		display: inline-flex;
		flex-direction: column;
		align-items: flex-start;
		gap: 22rpx;
}
.text1{
		color: #000;
		font-family: ABeeZee;
		font-size: 36rpx;
		font-style: normal;
		font-weight: 600;
		line-height: 32rpx; /* 88.889% */
}
.text2{
		color: #8B8F92;
		font-family: ABeeZee;
		font-size: 24rpx;
		font-style: normal;
		font-weight: 400;
		line-height: 32rpx; /* 133.333% */
}
.button_item image{
		position: absolute;
		bottom: 0;  /* 贴在上边 */
		    right: 0;  /* 贴在右边 */
		width: 300rpx;
		height: 300rpx;
		flex-shrink: 0;
}

